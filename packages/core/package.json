{
    "name": "@primereact/core",
    "version": "12.0.0",
    "author": "PrimeTek Informatics",
    "description": "",
    "homepage": "https://primereact.org/",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/primefaces/primereact.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/primefaces/primereact/issues"
    },
    "main": "./src/index.ts",
    "exports": {
        "./component": "./src/component/index.ts",
        "./basestyle": "./src/basestyle/index.ts",
        "./utils": "./src/utils/index.ts"
    },
    "publishConfig": {
        "main": "./index.mjs",
        "types": "./index.d.ts",
        "exports": {
            ".": {
                "types": "./index.d.ts",
                "import": "./index.mjs"
            },
            "./*": {
                "types": "./*/index.d.ts",
                "import": "./*/index.mjs"
            }
        },
        "directory": "dist",
        "linkDirectory": false,
        "access": "public"
    },
    "scripts": {
        "build": "NODE_ENV=production INPUT_DIR=src/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && rollup -c && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "dev:link": "pnpm link --global && npm link"
    },
    "dependencies": {
        "@primeuix/styled": "^0.0.5",
        "@primeuix/utils": "^0.0.5",
        "@primereact/hooks": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "engines": {
        "node": ">=12.11.0"
    }
}
